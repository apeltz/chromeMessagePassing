{
  "manifest_version": 2,
  "name": "Example Extension",
  "version": "1.0",

  "background": {
    "scripts": ["jquery-2.2.2.min.js", "background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-2.2.2.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  }
}
